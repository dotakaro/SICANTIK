				<textElement>
					<font size="11"/>
				</textElement>
				<text><![CDATA[Tidak melakukan perubahan pemegang izin tanpa persetujuan pemberi izin;]]></text>
			</staticText>
			<staticText>
				<reportElement uuid="3947b8e2-c886-4be1-a469-8298d95aafb0" x="31" y="572" width="14" height="20"/>
